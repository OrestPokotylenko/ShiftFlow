<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ViewModel;assembly=ViewModel"
                    xmlns:ui="clr-namespace:UI.SecondaryViews">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <ui:HomeViewEmployee />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CalendarVM}">
        <ui:CalendarView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ReplaceVM}">
        <ui:ReplaceView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:VacationVM}">
        <ui:VacationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfileVM}">
        <ui:ProfileView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingsVM}">
        <ui:SettingsView />
    </DataTemplate>

</ResourceDictionary>